{
  "name": "VidyoConnector",
  "version": "1.0.0",
  "productName": "Vidyo app",
  "description": "Multiparty HD Video Conferencing using Vidyo.io",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "compile-win": "node-gyp rebuild --target=14.2.9 --arch=x64 --dist-url=https://atom.io/download/electron -msvs_version=2017",
    "compile-mac": "node-gyp rebuild --target=14.2.9 --arch=$npm_config_arch --dist-url=https://atom.io/download/electron",
    "preinstall": "echo preinstalling ....",
    "postinstall": "npm run download-sdk-bundle && npm run compile-mac",
    "download-sdk-bundle": "node ./setup/"
  },
  "keywords": [
    "Electron",
    "Vidyo",
    "Connector",
    "demo"
  ],
  "author": "Vidyo",
  "devDependencies": {
    "electron": "^14.0.0",
    "node-gyp": "^3.8.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "cli-spinners": "^2.6.1",
    "decompress": "^4.2.1",
    "fs-extra": "^10.1.0",
    "nan": "^2.15.0",
    "progress-stream": "^2.0.0",
    "superagent": "^7.1.3"
  }
}
